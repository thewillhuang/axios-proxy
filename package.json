{
  "name": "mass-consensus",
  "version": "1.0.0",
  "main": "dist/main.js",
  "repository": "git@github.com:thewillhuang/mass-consensus.git",
  "author": "William Huang <will.h86@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn --ignore-engines && rm -fr dist && webpack --progress --mode=development --watch & yarn dev",
    "dev": "nodemon -e js,jsx,sql,json dist/main.js",
    "build": "yarn clean && yarn --ignore-engines && webpack --progress --mode=production && yarn nuke && yarn --prod --ignore-engines",
    "nuke": "rm -fr node_modules",
    "clean": "rm -fr dist",
    "stage": "yarn --ignore-engines && up && up url -o",
    "prod": "yarn --ignore-engines && up production && up url -o",
    "open": "up url -o",
    "logs": "up logs -f",
    "db:create": "createdb mass-consensus",
    "db:drop": "dropdb mass-consensus",
    "db:up": "db-migrate up",
    "db:r": "yarn db:down && yarn db:up",
    "db:down": "db-migrate down",
    "db:reset": "db-migrate reset"
  },
  "devDependencies": {
    "aws-sdk": "^2.209.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-transform-async-to-bluebird": "^1.1.1",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-morgan": "^1.0.1",
    "lodash": "^4.17.21",
    "nodemon": "^1.15.1",
    "ramda": "^0.25.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.6"
  }
}
